<p id="notice"><%= notice %></p>

<h1>Domains</h1>
<%= link_to 'log_out' ,destroy_user_session_path ,:method => :delete%>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Over all ranking</th>
      <th>Last update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @domains.each do |domain| %>
      <tr>
        <td><%= domain.name %></td>
        <td><%= domain.over_all_ranking %></td>
        <td><%= domain.last_update %></td>
        <td><%= link_to 'Show', domain %></td>
        <td><%= link_to 'Edit', edit_domain_path(domain) %></td>
        <td><%= link_to 'Destroy', domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Domain', new_domain_path %>
